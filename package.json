{
  "name": "material-design-wp-theme",
  "private": true,
  "title": "Material Design theme for WordPress",
  "license": "Apache-2.0",
  "homepage": "https://github.com/xwp/material-design-wp-theme#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xwp/material-design-wp-theme.git"
  },
  "bugs": {
    "url": "https://github.com/xwp/material-design-wp-theme/issues"
  },
  "engines": {
    "node": ">=10",
    "npm": ">=6.9"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@material/button": "5.1.0",
    "@material/card": "5.1.0",
    "@material/checkbox": "^5.1.0",
    "@material/drawer": "5.1.0",
    "@material/floating-label": "5.1.0",
    "@material/form-field": "5.1.0",
    "@material/icon-button": "5.1.0",
    "@material/image-list": "5.1.0",
    "@material/layout-grid": "5.1.0",
    "@material/list": "5.1.0",
    "@material/ripple": "5.1.0",
    "@material/tab-bar": "5.1.0",
    "@material/textfield": "5.1.0",
    "@material/top-app-bar": "5.1.0",
    "@material/typography": "5.1.0",
    "@wordpress/eslint-plugin": "4.0.0",
    "@wordpress/postcss-themes": "^2.6.0",
    "@wordpress/scripts": "^7.2.0",
    "babel-eslint": "10.0.3",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.6.0",
    "eslint": "6.8.0",
    "eslint-plugin-eslint-comments": "3.1.2",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-jest": "23.7.0",
    "eslint-plugin-react": "7.18.3",
    "eslint-utils": "2.0.0",
    "extract-loader": "^5.1.0",
    "grunt": "1.0.4",
    "grunt-contrib-clean": "2.0.0",
    "grunt-contrib-copy": "1.0.0",
    "grunt-shell": "3.0.1",
    "mini-css-extract-plugin": "^0.9.0",
    "npm-run-all": "4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "path": "^0.12.7",
    "postcss": "7.0.36",
    "postcss-calc": "7.0.2",
    "postcss-color-function": "^4.1.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-mixins": "6.2.3",
    "postcss-nested": "^4.2.3",
    "postcss-preset-env": "^6.7.0",
    "postcss-simple-vars": "5.0.2",
    "prettier": "npm:wp-prettier@1.19.1",
    "rtlcss-webpack-plugin": "^4.0.3",
    "terser-webpack-plugin": "^2.3.8",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-fix-style-only-entries": "^0.6.0",
    "webpackbar": "^4.0.0"
  },
  "scripts": {
    "build": "npm-run-all build:*",
    "build:prepare": "grunt clean",
    "build:js": "wp-scripts build && NODE_ENV=dev wp-scripts build",
    "build:run": "grunt build",
    "build:zip": "grunt create-build-zip",
    "dev": "wp-scripts start",
    "docker": "bin/docker.sh",
    "env:logs": "bin/logs.sh",
    "env:start": "bin/start.sh",
    "env:stop": "bin/stop.sh",
    "format": "npm-run-all --parallel format:*",
    "format:css": "npm run lint:css -- --fix",
    "format:js": "wp-scripts format-js",
    "format:php": "composer format",
    "lint": "npm-run-all --parallel lint:*",
    "lint:css": "wp-scripts lint-style",
    "lint:js": "wp-scripts lint-js",
    "lint:php": "composer lint",
    "plugin-dev": "bin/plugin-dev.sh",
    "postinstall": "composer install",
    "readme": "composer readme",
    "test": "npm-run-all --parallel test:php",
    "test:php": "composer test",
    "wp": "bin/wp.sh",
    "xdebug:status": "bin/xdebug.sh status",
    "xdebug:on": "bin/xdebug.sh on",
    "xdebug:off": "bin/xdebug.sh off"
  },
  "npmPackageJsonLintConfig": {
    "extends": "@wordpress/npm-package-json-lint-config",
    "rules": {
      "require-version": "off"
    }
  }
}
